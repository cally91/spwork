package com.biz.iolist.mapper;

import org.apache.ibatis.jdbc.SQL;

public class MemberSQL {

	public String member_insert_sql() {
		SQL sql =new SQL() {{
			INSERT_INTO("tbl_member");
			INTO_COLUMNS("m_userid").INSERT_INTO("#{m_userid,jdbcType=VARCHAR}");
			VALUES("m_password","#{m_password,jdbcType=VARCHAR}");
			VALUES("m_name,m_tel","#{m_password,jdbcType=VARCHAR},#{m_tel,jdbcType=VARCHAR}");
			INTO_COLUMNS("m_addr").INSERT_INTO("#{m_addr,jdbcType=VARCHAR}");
			INTO_COLUMNS("m_roll").INSERT_INTO("#{m_roll,jdbcType=VARCHAR}");
		}};
		
		return sql.toString();
	}
}
