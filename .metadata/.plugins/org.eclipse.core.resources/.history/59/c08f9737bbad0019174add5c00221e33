package com.biz.iolist.mapper;

import org.apache.ibatis.jdbc.SQL;

public class MemberSQL {

	public String member_insert_sql() {
		SQL sql =new SQL() {{
			INSERT_INTO("tbl_member");
			INTO_COLUMNS("m_userid").INSERT_INTO("#{m_userid}");
			VALUES("m_password","#{m_password}");
			VALUES("m_name","m_tel","#{m_password},#{m_tel}");
		}};
		
		return sql.toString();
	}
}
