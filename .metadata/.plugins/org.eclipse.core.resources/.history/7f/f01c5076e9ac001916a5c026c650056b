<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>

<table class="list">
	<tr>
		<th>CODE</th>
		<th>거래처명</th>
		<th>대표</th>
		<th>전화번호</th>
		<th>주소</th>
	</tr>
	<c:choose>
		<c:when test="${empty COMP_LIST}">
		<tr> 
		<td colspan="3">데이터가 없습니다</td>
		</tr>
		</c:when>
	<c:otherwise>
	<c:forEach items="${COMP_LIST}" var="cVO"> 
	<tr>
	<td>${cVO.c_code}</td>
	<td>${cVO.c_name}</td>
	<td>${cVO.c_ceo}</td>
	<td>${cVO.c_tel}</td>
	<td>${cVO.c_addr}</td>
	</tr>
	</c:forEach>
	
	</c:otherwise>
	</c:choose>
</table>