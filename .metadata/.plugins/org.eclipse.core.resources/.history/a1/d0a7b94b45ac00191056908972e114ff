package com.biz.lolist.mapper;

import org.apache.ibatis.jdbc.SQL;

public class IolistSQL {

	public String iolist_insert_sql() {
		SQL	sql=new SQL() ;
		sql.INSERT_INTO("tbl_iolist");
		sql.INTO_COLUMNS("io_seq").INTO_VALUES("#{io_seq}");
		sql.INTO_COLUMNS("io_date").INTO_VALUES("#{io_date}");
		sql.INTO_COLUMNS("io_product").INTO_VALUES("#{io_product}");
		sql.INTO_COLUMNS("io_comp").INTO_VALUES("#{io_comp}");
		sql.INTO_COLUMNS("io_com_ceo").INTO_VALUES("#{io_com_ceo}");
		sql.INTO_COLUMNS("io_inout").INTO_VALUES("#{io_inout}");
		sql.INTO_COLUMNS("io_qty").INTO_VALUES("#{io_qty}");
		sql.INTO_COLUMNS("io_price").INTO_VALUES("#{io_price}");
		sql.INTO_COLUMNS("io_total").INTO_VALUES("#{io_total}");
		return sql.toString();
	}
}
