package com.biz.iolist.mapper;

import org.apache.ibatis.annotations.InsertProvider;
import org.apache.ibatis.annotations.Select;

import com.biz.iolist.Model.MemberVO;

public interface MemberDao {

	@Select("SELECT m_userid " + "FROM tbl_member WHERE m_userid = #{m_userid}")
	public String check_id(String m_userid);

	@InsertProvider(MemderSQL.class,method="member_insert_sql")
	public int insert(MemberVO memberVO);
}
