$(function() {
	
	$("btn-save").click(function() {
	// 유효성 검사
	if($("#io_date").val() == ""){
		alert("거래일자를 입력하세요")
		$("#io_date").focus()
		return false
	}
	
	if($(":radio[name='io_inout':chcked").length < 1){
		alert(" 매입매출을 입력하세요")
		$("#io_inout").focus()
		return false
	}
	
		if($("#io_ccode").val()==""){
		
		}
	})
})
